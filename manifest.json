{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery-1.10.2.js", "js/jquery-ui.js", "js/mycode.js", "js/shortcut.js" ],
      "matches": [ "http://*/*", "https://*/*", "file://*" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": "script-src 'self' https://www.google.com https://secure.flickr.com; object-src 'self'",
   "description": "Highlights, annotates, translates, full or quick references (eg: wordreference.com, wikipedia), speech, and more",
   "icons": {
      "128": "img/128.png",
      "16": "img/16.png",
      "48": "img/48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18",
   "name": "Wordy Highlighter",
   "options_page": "options.html",
 "browser_action": {
     "default_title": "Wordy Highligher",
     "default_icon": "img/unlocked.png" //<--this line: change "star-on.png" to the icon you want
   },
